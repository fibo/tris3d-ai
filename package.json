{
  "name": "tris3d-ai",
  "description": "tris3d surrogate player",
  "version": "0.1.0",
  "homepage": "http://npm.im/tris3d-ai",
  "author": {
    "name": "Gianluca Casati",
    "url": "http://g14n.info"
  },
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "check-deps": "npm outdated",
    "lint": "standard",
    "postupdate-deps": "npm test && git commit -am 'updated deps' || git checkout -- package.json",
    "postversion": "git push origin v${npm_package_version}; npm publish; git push origin master",
    "test": "NODE_PATH=src tape test/*js | tap-min",
    "update-deps": "npm update --save --dev"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/fibo/tris3d-ai.git"
  },
  "keywords": [
    "tris3d"
  ],
  "bugs": {
    "url": "https://github.com/fibo/tris3d-ai/issues"
  },
  "pre-commit": [
    "lint",
    "test",
    "check-deps"
  ],
  "devDependencies": {
    "pre-commit": "^1.1.2",
    "standard": "^7.0.0",
    "tap-min": "^1.1.0",
    "tape": "^4.5.0"
  },
  "dependencies": {
    "standard": "^7.1.2",
    "tris3d": "^0.3.0"
  }
}
